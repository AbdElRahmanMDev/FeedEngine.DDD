{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },

    "WriteTo": [
      { "Name": "Console" },

      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "ConnectionStrings": {
    "AppDb": "Server=(localdb)\\mssqllocaldb;Database=FeedBuilderDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "FeedEngine",
    "Audience": "FeedEngine",
    "SecretKey": "v3kbl9qvSHgvuOZYfW4ZL8KmVduZXGh1",
    "ExpiresInMinutes": 60
  },
  "AllowedHosts": "*"
}
